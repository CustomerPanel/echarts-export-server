{
  "name": "echarts-export-server",
  "version": "1.0.1",
  "description": "Apache ECharts HTTP Export Server",
  "main": "src/index.js",
  "scripts": {
    "start": "pm2 start ./src/index.js --name echarts-export-server",
    "stop": "pm2 stop echarts-export-server",
    "logs": "pm2 logs echarts-export-server",
    "status": "pm2 list",
    "show": "pm2 show echarts-export-server",
    "delete": "pm2 delete echarts-export-server",
    "foreground": "node ./src/index.js",
    "dev": "nodemon ./src/index.js"
  },
  "dependencies": {
    "canvas": "^2.11.2",
    "date-fns": "^3.6.0",
    "echarts": "5.5.1",
    "ejs": "^3.1.9",
    "node-html-to-image": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "Apache",
    "ECharts",
    "HTTP",
    "Export",
    "Server",
    "Java",
    "Visualization"
  ],
  "author": "xiaomaiyun",
  "license": "Apache License 2.0"
}
